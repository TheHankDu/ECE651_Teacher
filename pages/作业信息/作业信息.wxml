<!--pages/作业信息/作业信息.wxml-->
<view class="doc-title" wx:for="{{AssignmentArray}}">
  <text class="list_title" catchtap="View" data-assignment="{{item}}">{{item.title}} deadline: {{item.deadline}}</text>
  <view class="hr"></view>
</view>

<view>
  <button class="Add" ontap="Add">{{Add}}</button>
</view>


<view>
  <button class="Home" ontap="Home">Return To Home</button>
</view>

 
   